{
  "name": "facservice",
  "version": "1.0.0",
  "description": "FacService Web Apps for ISSATSo students",
  "repository": "https://gitlab.com/igcforces/project-one-web-backend.git",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "seeds": "md-seed run",
    "start": "babel-node ./src/",
    "lint": "eslint --fix **/*.js",
    "docs": "node ./node_modules/apidoc/bin/apidoc -i src/ -o public/apidoc/",
    "build": "node ./node_modules/babel-cli/bin/babel.js src --out-dir build --copy-files",
    "update": "git pull origin master;npm run build; pm2 restart facservice; pm2 logs"
  },
  "apidoc": {
    "title": "Custom apiDoc browser title",
    "url": "https://api.facservice.tn",
    "template": {
      "forceLanguage": "en"
    }
  },
  "author": "",
  "license": "ISC",
